# プロジェクト設定
project_id  = "your-gcp-project-id"
region      = "asia-northeast1"
environment = "prod"

# Cloud Run 設定
cloud_run_service_name = "seo-writer"
cloud_run_min_instances = 0
cloud_run_max_instances = 10
cloud_run_cpu           = "1"
cloud_run_memory        = "512Mi"
cloud_run_timeout       = 300

# Cloud SQL 設定
db_instance_name = "seo-writer-db"
db_name          = "seo_writer"
db_user          = "seo_writer"
db_tier          = "db-f1-micro"  # 本番環境では db-custom-1-3840 以上を推奨

# Artifact Registry 設定
artifact_registry_repository = "seo-writer"

# API Keys
gemini_api_key = "your-gemini-api-key"
